CREATE TABLE tickets(
  id int NOT NULL AUTO_INCREMENT PRIMARY KEY ,
  ticket_number VARCHAR (50),
  ticket_comfortlevel int,
  ticket_idclient int,
  ticket_idcruise int
);

INSERT INTO tickets(ticket_number, ticket_comfortlevel, ticket_idclient, ticket_idcruise) VALUES ('111',1,1,3);
