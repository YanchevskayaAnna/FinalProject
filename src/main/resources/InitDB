CREATE TABLE cruises(
  id int NOT NULL AUTO_INCREMENT PRIMARY KEY ,
  cruise_name VARCHAR (50),
  cruise_number VARCHAR (20),
  cruise_idShip int,
  cruise_dateStart date,
  cruise_dateFinish date,
  cruise_price int,
  cruise_countdays int
);

