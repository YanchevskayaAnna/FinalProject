CREATE DATABASE TravelCompany;
USE TravelCompany;

CREATE TABLE clients(
  id int NOT NULL AUTO_INCREMENT PRIMARY KEY ,
  client_name VARCHAR (20),
  client_phone VARCHAR (20),
  client_mail VARCHAR (50)
);

